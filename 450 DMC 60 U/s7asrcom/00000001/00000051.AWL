FUNCTION "FC_SPL_ZD_BER1_MASCH" : VOID
TITLE =
//$Revision: 1.11 $
//$Date: 2008/06/16 11:06:55CEST $
//$Author: hgc $
//
//Baustein für Solutionline DMx55-125x2
//Zwangsdynamisierung Bereich Maschine
AUTHOR : AB
VERSION : 0.0


VAR_INPUT
  I_BT_Anfordern : BOOL ;	//Bremsentest anfordern
  I_BT_2_Anfordern : BOOL ;	
  I_ZD_Anfordern : BOOL ;	//Zwangsdynamisierung anfordern
  I_HW_Anfordern : BOOL ;	//Hardware testen
END_VAR
VAR_IN_OUT
  O_BT_Laeuft : BOOL ;	
  O_BT_2_Laeuft : BOOL ;	
  O_BT_Error : BOOL ;	//Fehler BT --> Bremse defekt
  O_BT_2_Error : BOOL ;	
  O_ZD_Laeuft : BOOL ;	
  O_ZD_SError : BOOL ;	//Fehler ZD Startbedingungen
  O_ZD_Error : BOOL ;	//Fehler Zwangsdynamisierung
  O_Bremse_defekt : BOOL ;	
  O_Bremse_2_defekt : BOOL ;	
  O_BT_Error_FC18 : BOOL ;	//Fehler beim Bremsentest mit FC18
  O_BT_Error_2_FC18 : BOOL ;	
  O_HW_laeuft : BOOL ;	
END_VAR
VAR_TEMP
  T_fnum : BYTE ;	
  T_Bit : BYTE ;	
  T_Byte : BYTE ;	
  T_errword : WORD ;	
  T_Err_1Eingang : BOOL ;	//Fehler Nur 1 Eingang darf 1 sein
  T_Err_BA1 : BOOL ;	//Fehler Test Freigabe Drehzahl 
  T_Err_BA2 : BOOL ;	//Fehler Test BA 2
  T_Err_BA3 : BOOL ;	//Fehler Test BA 3
  T_Err_BA4 : BOOL ;	//Fehler Test BA 4
  T_test : BOOL ;	
  T_ANZAHL_ARB_ACHSEN : INT ;	//Anzahl der Arbeitsraumachsen
  T_ANZAHL_ZUSATZ_ACHSEN : INT ;	//Anzahl der Werkzeugwechslerachsen
  T_ZUSATZ_ACHSE_1_NUMMER : INT ;	//Achsnummer der Zusatzachse 1
  T_ZUSATZ_ACHSE_2_NUMMER : INT ;	//Achsnummer der Zusatzachse 2
  T_ZUSATZ_ACHSE_3_NUMMER : INT ;	//Achsnummer der Zusatzachse 3
  T_ZUSATZ_ACHSE_4_NUMMER : INT ;	//Achsnummer der Zusatzachse 4
  T_ZUSATZ_ACHSE_5_NUMMER : INT ;	//Achsnummer der Zusatzachse 5
  T_ZUSATZ_ACHSE_6_NUMMER : INT ;	//Achsnummer der Zusatzachse 6
  FC_SPL_ZD1_SERROR1 : BOOL ;	
  FC_SPL_ZD1_SERROR2 : BOOL ;	
  FC_SPL_ZD1_ERROR1 : BOOL ;	
  FC_SPL_ZD1_ERROR2 : BOOL ;	
  FC_SPL_ZD2_SERROR1 : BOOL ;	
  FC_SPL_ZD2_SERROR2 : BOOL ;	
  FC_SPL_ZD2_ERROR1 : BOOL ;	
  FC_SPL_ZD2_ERROR2 : BOOL ;	
  S3_READY : BOOL ;	
  S4_START : BOOL ;	
  S5_READY : BOOL ;	
  S6_START : BOOL ;	
  T_MASCH_MIT_5_ARB_ACHSEN : BOOL ;	
  T_ARB_ACHSE_6_NUMMER : INT ;	
END_VAR
BEGIN
NETWORK
TITLE =Maschine mit 5 Arbeitsraumachsen
//- H-Maschine ohne Teilapperat und ohne Schwenkrundtisch
//- P/U-MAschiine ohne B-Kopfachse
      U     "DB_CONFIG".ANWAHL_DURCH_USER_DATA.MASCHTYP_3071_AKTIV; 
      U(    ; 
      L     "DB_PLC_MD_DB20".UDInt._043_ANWAHL_TISCH; 
      L     1; 
      <>I   ; 
      )     ; 
      U(    ; 
      L     "DB_PLC_MD_DB20".UDInt._043_ANWAHL_TISCH; 
      L     2; 
      <>I   ; 
      )     ; 
      O     ; 
      U     "DB_CONFIG".ANWAHL_DURCH_USER_DATA.MASCHTYP_3070_AKTIV; 
      U(    ; 
      L     "DB_PLC_MD_DB20".UDInt._007_ANWAHL_FRAESKOPF; 
      L     1; 
      ==I   ; 
      )     ; 
      =     #T_MASCH_MIT_5_ARB_ACHSEN; 
NETWORK
TITLE =Anzahl der Achsen ermitteln
//Der Zwangsdynamisierungsbereich 1 Maschine besteht aus:
//- den Arbeitsraumachsen
//- den WZW-Achsen
//- der PW-Achse
//
//
// *** Anzahl der Arbeitsraumachsen **********************
// *******************************************************
      U     #T_MASCH_MIT_5_ARB_ACHSEN; 
      SPB   AAX5; 

// ZD nicht ausgelegt für 7 Arbeitsraumachse
      U     "DB_SIEM_STARTUP".ActivAxis[5]; 
      U     "DB_SIEM_STARTUP".ActivAxis[11]; 
      SPB   FAUL; 

// Achsnummer der 6.ten Arbeitsraumachse ermitteln
      U     "DB_SIEM_STARTUP".ActivAxis[5]; //B-Kopf oder Schwenkrundtisch
      SPBN  ANR1; 
      L     5; 
      SPB   AEND; 
ANR1: U     "DB_SIEM_STARTUP".ActivAxis[11]; // NC-Teilapperat
      SPBN  FAUL; 
      L     11; 
AEND: T     #T_ARB_ACHSE_6_NUMMER; 
      SPA   KFAU; 

// Software nicht ausgelegt für diese Maschinenkonfiguration
FAUL: SET   ; 
      S     "DB_FEHLERMELDUNG".NotAus._700063_SW_CONFIG_FAULT; 
      SPA   ZAX; 

KFAU: L     6; 
      SPA   AAX6; 

AAX5: L     5; 
AAX6: T     #T_ANZAHL_ARB_ACHSEN; 

// *** Anzahl der Zusatzachsen (WZW / PW)*****************
// *******************************************************
// --- Konfigurationen mit PW-Achse ----------------------
// MAG (Master/Slave) + GREIFER + PW Achse
      U     "DB_SIEM_STARTUP".ActivAxis[7]; 
      U     "DB_SIEM_STARTUP".ActivAxis[13]; 
      U     "DB_SIEM_STARTUP".ActivAxis[14]; 
      U     "DB_SIEM_STARTUP".ActivAxis[8]; 
      SPBN  ZAX1; 
      L     4; 
      T     #T_ANZAHL_ZUSATZ_ACHSEN; 

      L     7; 
      T     #T_ZUSATZ_ACHSE_1_NUMMER; 
      L     13; 
      T     #T_ZUSATZ_ACHSE_2_NUMMER; 
      L     14; 
      T     #T_ZUSATZ_ACHSE_3_NUMMER; 
      L     8; 
      T     #T_ZUSATZ_ACHSE_4_NUMMER; 
      L     0; 
      T     #T_ZUSATZ_ACHSE_5_NUMMER; 
      T     #T_ZUSATZ_ACHSE_6_NUMMER; 
      SPA   ZAX; 

// MAG (Master) + GREIFER + PW Achse
ZAX1: U     "DB_SIEM_STARTUP".ActivAxis[7]; 
      UN    "DB_SIEM_STARTUP".ActivAxis[13]; 
      U     "DB_SIEM_STARTUP".ActivAxis[14]; 
      U     "DB_SIEM_STARTUP".ActivAxis[8]; 
      SPBN  ZAX2; 
      L     3; 
      T     #T_ANZAHL_ZUSATZ_ACHSEN; 

      L     7; 
      T     #T_ZUSATZ_ACHSE_1_NUMMER; 
      L     14; 
      T     #T_ZUSATZ_ACHSE_2_NUMMER; 
      L     8; 
      T     #T_ZUSATZ_ACHSE_3_NUMMER; 
      L     0; 
      T     #T_ZUSATZ_ACHSE_4_NUMMER; 
      T     #T_ZUSATZ_ACHSE_5_NUMMER; 
      T     #T_ZUSATZ_ACHSE_6_NUMMER; 
      SPA   ZAX; 

// MAG (Master) + PW Achse
ZAX2: U     "DB_SIEM_STARTUP".ActivAxis[7]; 
      UN    "DB_SIEM_STARTUP".ActivAxis[13]; 
      UN    "DB_SIEM_STARTUP".ActivAxis[14]; 
      U     "DB_SIEM_STARTUP".ActivAxis[8]; 
      SPBN  ZAX3; 
      L     2; 
      T     #T_ANZAHL_ZUSATZ_ACHSEN; 

      L     7; 
      T     #T_ZUSATZ_ACHSE_1_NUMMER; 
      L     8; 
      T     #T_ZUSATZ_ACHSE_2_NUMMER; 
      L     0; 
      T     #T_ZUSATZ_ACHSE_3_NUMMER; 
      T     #T_ZUSATZ_ACHSE_4_NUMMER; 
      T     #T_ZUSATZ_ACHSE_5_NUMMER; 
      T     #T_ZUSATZ_ACHSE_6_NUMMER; 
      SPA   ZAX; 

// MAG (Master/Slave) + GREIFER + PW Achse + RPL-Achse
ZAX3: U     "DB_SIEM_STARTUP".ActivAxis[7]; 
      U     "DB_SIEM_STARTUP".ActivAxis[13]; 
      U     "DB_SIEM_STARTUP".ActivAxis[14]; 
      U     "DB_SIEM_STARTUP".ActivAxis[8]; 
      U     "DB_SIEM_STARTUP".ActivAxis[15]; 
      SPBN  ZAX4; 
      L     5; 
      T     #T_ANZAHL_ZUSATZ_ACHSEN; 

      L     7; 
      T     #T_ZUSATZ_ACHSE_1_NUMMER; 
      L     13; 
      T     #T_ZUSATZ_ACHSE_2_NUMMER; 
      L     14; 
      T     #T_ZUSATZ_ACHSE_3_NUMMER; 
      L     8; 
      T     #T_ZUSATZ_ACHSE_4_NUMMER; 
      L     15; 
      T     #T_ZUSATZ_ACHSE_5_NUMMER; 
      L     0; 
      T     #T_ZUSATZ_ACHSE_6_NUMMER; 
      SPA   ZAX; 

// MAG (Master) + GREIFER + PW Achse + RPL-Achse
ZAX4: U     "DB_SIEM_STARTUP".ActivAxis[7]; 
      UN    "DB_SIEM_STARTUP".ActivAxis[13]; 
      U     "DB_SIEM_STARTUP".ActivAxis[14]; 
      U     "DB_SIEM_STARTUP".ActivAxis[8]; 
      U     "DB_SIEM_STARTUP".ActivAxis[15]; 
      SPBN  ZAX5; 
      L     4; 
      T     #T_ANZAHL_ZUSATZ_ACHSEN; 

      L     7; 
      T     #T_ZUSATZ_ACHSE_1_NUMMER; 
      L     14; 
      T     #T_ZUSATZ_ACHSE_2_NUMMER; 
      L     8; 
      T     #T_ZUSATZ_ACHSE_3_NUMMER; 
      L     15; 
      T     #T_ZUSATZ_ACHSE_4_NUMMER; 
      L     0; 
      T     #T_ZUSATZ_ACHSE_5_NUMMER; 
      T     #T_ZUSATZ_ACHSE_6_NUMMER; 
      SPA   ZAX; 

// MAG (Master) + PW Achse + RPL-Achse
ZAX5: U     "DB_SIEM_STARTUP".ActivAxis[7]; 
      UN    "DB_SIEM_STARTUP".ActivAxis[13]; 
      UN    "DB_SIEM_STARTUP".ActivAxis[14]; 
      U     "DB_SIEM_STARTUP".ActivAxis[8]; 
      U     "DB_SIEM_STARTUP".ActivAxis[15]; 
      SPBN  ZAX6; 
      L     3; 
      T     #T_ANZAHL_ZUSATZ_ACHSEN; 

      L     7; 
      T     #T_ZUSATZ_ACHSE_1_NUMMER; 
      L     8; 
      T     #T_ZUSATZ_ACHSE_2_NUMMER; 
      L     15; 
      T     #T_ZUSATZ_ACHSE_3_NUMMER; 
      L     0; 
      T     #T_ZUSATZ_ACHSE_4_NUMMER; 
      T     #T_ZUSATZ_ACHSE_5_NUMMER; 
      T     #T_ZUSATZ_ACHSE_6_NUMMER; 
      SPA   ZAX; 

// --- Konfigurationen ohne PW-Achse -------------------------
// MAG (Master)
ZAX6: U     "DB_SIEM_STARTUP".ActivAxis[7]; 
      UN    "DB_SIEM_STARTUP".ActivAxis[13]; 
      UN    "DB_SIEM_STARTUP".ActivAxis[14]; 
      UN    "DB_SIEM_STARTUP".ActivAxis[8]; 
      SPBN  ZAX7; 
      L     1; 
      T     #T_ANZAHL_ZUSATZ_ACHSEN; 

      L     7; 
      T     #T_ZUSATZ_ACHSE_1_NUMMER; 
      L     0; 
      T     #T_ZUSATZ_ACHSE_2_NUMMER; 
      T     #T_ZUSATZ_ACHSE_3_NUMMER; 
      T     #T_ZUSATZ_ACHSE_4_NUMMER; 
      T     #T_ZUSATZ_ACHSE_5_NUMMER; 
      T     #T_ZUSATZ_ACHSE_6_NUMMER; 
      SPA   ZAX; 

// MAG (Master) + GREIFER
ZAX7: U     "DB_SIEM_STARTUP".ActivAxis[7]; 
      UN    "DB_SIEM_STARTUP".ActivAxis[13]; 
      U     "DB_SIEM_STARTUP".ActivAxis[14]; 
      UN    "DB_SIEM_STARTUP".ActivAxis[8]; 
      SPBN  ZAX8; 
      L     2; 
      T     #T_ANZAHL_ZUSATZ_ACHSEN; 

      L     7; 
      T     #T_ZUSATZ_ACHSE_1_NUMMER; 
      L     14; 
      T     #T_ZUSATZ_ACHSE_2_NUMMER; 
      L     0; 
      T     #T_ZUSATZ_ACHSE_3_NUMMER; 
      T     #T_ZUSATZ_ACHSE_4_NUMMER; 
      T     #T_ZUSATZ_ACHSE_5_NUMMER; 
      T     #T_ZUSATZ_ACHSE_6_NUMMER; 
      SPA   ZAX; 

// MAG (Master/Slave) + GREIFER
ZAX8: U     "DB_SIEM_STARTUP".ActivAxis[7]; 
      U     "DB_SIEM_STARTUP".ActivAxis[13]; 
      U     "DB_SIEM_STARTUP".ActivAxis[14]; 
      UN    "DB_SIEM_STARTUP".ActivAxis[8]; 
      SPBN  ZAX; 
      L     3; 
      T     #T_ANZAHL_ZUSATZ_ACHSEN; 

      L     7; 
      T     #T_ZUSATZ_ACHSE_1_NUMMER; 
      L     13; 
      T     #T_ZUSATZ_ACHSE_2_NUMMER; 
      L     14; 
      T     #T_ZUSATZ_ACHSE_3_NUMMER; 
      L     0; 
      T     #T_ZUSATZ_ACHSE_4_NUMMER; 
      T     #T_ZUSATZ_ACHSE_5_NUMMER; 
      T     #T_ZUSATZ_ACHSE_6_NUMMER; 

ZAX:  NOP   0; 
NETWORK
TITLE =Lokalvariablen ablöschen

      CLR   ; 
      =     #FC_SPL_ZD1_SERROR1; 
      =     #FC_SPL_ZD1_SERROR2; 
      =     #FC_SPL_ZD1_ERROR1; 
      =     #FC_SPL_ZD1_ERROR2; 
      =     #FC_SPL_ZD2_SERROR1; 
      =     #FC_SPL_ZD2_SERROR2; 
      =     #FC_SPL_ZD2_ERROR1; 
      =     #FC_SPL_ZD2_ERROR2; 

NETWORK
TITLE = Test - Anstoss BA 1

      U     #I_HW_Anfordern; 
      FP    "DB_SAFE_ZD".Seco_Test.FP_BA1; 
      =     #O_HW_laeuft; 
NETWORK
TITLE =Test der mechanischen Funktion der Bremse vertikale Achse

      U     "Mx_Vert_Bremse_auf"; 
      UN    "DB_SAFE_ZD".GR1_ZD.Bremse; // Zwangsdynamisierung
      UN    "DB_SAFE_ZD".GR1_BT.Bremse_schliessen; 
      NOT   ; 
      =     "DB_SAFE_ZD".GR1_BT.Bclosed_FB11; 

      U     #I_BT_Anfordern; //  Anstoss fuer den Bremsentest vertikale-Achse
      FP    "DB_SAFE_ZD".GR1_BT.BT_Start_Taste_FP; 
      UN    "DB_SAFE_ZD".GR1_BT.BT_error; //Fehler aufgetreten
      S     "DB_SAFE_ZD".GR1_BT.BT_laeuft; //Bremsentest laeuft
      S     "DB_SAFE_ZD".GR1_BT.Schritt1; //naechster Schritt
      S     "DB_ACHSE3_Z".A_NCASpStrobe; //Neutrale Achse anfordern
      R     "DB_ACHSE3_Z".A_NCASpChanA; //Neutrale Achse anfordern
      S     #O_BT_Laeuft; //Bremsentest laeuft

      U     "DB_ACHSE3_Z".E_NeutrASp; //Rueckmeldung Achse ist neutral
      U     "DB_SAFE_ZD".GR1_BT.Schritt1; 
      FP    "DB_SAFE_ZD".GR1_BT.Schritt1_FP; 
      S     "DB_SAFE_ZD".GR1_BT.Schritt2; //naechster Schritt
      R     "DB_SAFE_ZD".GR1_BT.Schritt1; 
      R     "DB_ACHSE3_Z".A_NCASpStrobe; 
      S     "DB_ACHSE3_Z".A_PLCAxis; //PLC-kontrollierte Achse anfordern (MD 10008)

      U     "DB_ACHSE3_Z".E_PLCCtrlAx; //Rueckmeldung Achse wird von PLC kontrolliert
      U     "DB_SAFE_ZD".GR1_BT.Schritt2; 
      FP    "DB_SAFE_ZD".GR1_BT.Schtitt2_FP; 
      R     "DB_SAFE_ZD".GR1_BT.Schritt2; 
      S     "DB_SAFE_ZD".GR1_BT.Start_BT_FB11; //Start Bremsentest fuer FB

      CALL "SI_BrakeTest" , "DB_SAFE_BTGR1_INST" (
           Start                    := "DB_SAFE_ZD".GR1_BT.Start_BT_FB11,
           Quit                     := "M_Ruecksetze_Fehler",//"MMC_SIGNALS".E_Cancel
           Bclosed                  := "DB_SAFE_ZD".GR1_BT.Bclosed_FB11,
           Axis                     := 3,
           TimerNo                  := "T_SAFECO_Bremsentest_1",
           TV_BTactiv               := S5T#200MS,//400ms
           TV_Bclose                := S5T#1S,//500MS
           TV_FeedCommand           := S5T#1S,//500MS
           TV_FXSreached            := S5T#1S,//500MS
           TV_FXShold               := S5T#1S500MS,//500MS
           CloseBrake               := "DB_SAFE_ZD".GR1_BT.Bremse_schliessen,
           MoveAxis                 := "DB_SAFE_ZD".GR1_BT.MoveAxis_FB11,
           Done                     := "DB_SAFE_ZD".GR1_BT.Done_FB11,
           Error                    := "DB_SAFE_ZD".GR1_BT.BT_error,
           State                    := "DB_SAFE_ZD".GR1_BT.State_FB11);

// S t a t e
// -----------
// 0:Kein Fehler                                                                                                            
// 1:Startbedingungen nicht gegeben, z.B. Achse nicht in Regelung / Bremse zu / Achsensperre steht an.                      
// 2:Bei Anwahl Bremsentest erfolgt keine Rückmeldung der NC im Signal "Bremsentest aktiv"                                  
// 3:Keine Rückmeldung "Bremse eingefallen" durch Eingangssignal BClosed                                                    
// 4:Keine Fahrbefehlausgabe (z.B. Bewegung der Achse wurde nicht gestartet)                                                
// 5:Festanschlag wird nicht erreicht -> Achs-RESET wurde ausgelöst                                                         
// 6:Verfahrsperre/Anfahrt zu langsam -> Festanschlag kann nicht erreicht werden. Überwachungszeit TV_FXSreached abgelaufen.
// 7:Bremse hält überhaupt nicht (Endposition wird erreicht) / Anfahrgeschwindigkeit zu groß                                
// 8:Bremse bricht während der Haltezeit auf                                                                                
// 9:Fehler bei Bremsentest-Abwahl                                                                                          
//10:Interner Fehler                                                                                                        
//11:Signal "PLC kontrolliert Achse" nicht eingeschaltet vom Anwenderprogramm (MD 10008)


      U     "M_Ruecksetze_Fehler"; 
      SPBN  er1; 
      L     0; 
      T     #T_errword; 
      R     "DB_SAFE_ZD".GR1_BT.BT_error; 
      SPA   equt; 

er1:  U     "DB_SAFE_ZD".GR1_BT.BT_error; 
      SPBN  eend; 
      L     "DB_SAFE_ZD".GR1_BT.State_FB11; 
      T     #T_fnum; 
      L     0; 
      ==I   ; 
      SPB   eend; 
      L     0; 
      T     #T_errword; 
      L     #T_fnum; 
      DEC   1; 
      L     8; 
      MOD   ; 
      T     #T_Bit; 
      L     #T_fnum; 
      L     8; 
      /I    ; 
      T     #T_Byte; 
      SLD   3; 
      L     #T_Bit; 
      OW    ; 
      LAR1  ; 
      L     P##T_errword; 
      +AR1  ; 
      S     L [AR1,P#0.0]; 
equt: L     #T_errword; 
      AUF   "DB_FEHLERMELDUNG"; 
      L     P#DBX 114.0; 
      LAR1  ; 
      L     #T_errword; 
      T     DBW [AR1,P#0.0]; 
eend: NOP   0; 


// Testmoment wird über MD 36966 definiert / Toleranz im MD 36967
// Beispiel: Haltemoment für Z-Achse 28% (MD 1728)
// Testmoment |50%| -> Achsgewicht (28%) ==> Motor (-23%) : MD 1728

      O     "DB_SAFE_ZD".GR1_BT.Done_FB11; //Test erfolgreich beendet
      O     "DB_SAFE_ZD".GR1_BT.BT_error; //Fehler aufgetreten
      FP    "DB_SAFE_ZD".GR1_BT.Done_Error_FB11_FP; 
      R     "DB_ACHSE3_Z".A_PLCAxis; //Anforderung PLC-kontrollierte Achse rücksetzen

      UN    "DB_ACHSE3_Z".E_PLCCtrlAx; //Rueckmeldung Achse wird von PLC kontrolliert
      U     "DB_SAFE_ZD".GR1_BT.Start_BT_FB11; //Start Bremsentest fuer FB
      U     "DB_SAFE_ZD".GR1_BT.BT_laeuft; //Bremsentest laeuft
      FP    "DB_SAFE_ZD".GR1_BT.BT_laeuft_PLCAx_FP; 
      R     "DB_SAFE_ZD".GR1_BT.Start_BT_FB11; //Start Bremsentest fuer FB
      R     "DB_SAFE_ZD".GR1_BT.BT_laeuft; //Bremsentest laeuft

      CALL "FC_SIEM_SPINCTRL" (//Z-Achse verfahren
           Start                    := "DB_SAFE_ZD".GR1_BT.MoveAxis_FB11,//Start des Verfahrbewegung
           Stop                     := FALSE,
           Funct                    := B#16#5,//Mode: Achsbetrieb
           Mode                     := B#16#1,//Verfahren: Inkrementell
           AxisNo                   := 3,//Achsnummer der zu verfahrenden Achse Z-Achse
           Pos                      := -3.000000e+000,//Verfahrweg: minus 3mm
           FRate                    := 2.000000e+003,//Vorschub: 2000 mm/min
           InPos                    := "DB_SAFE_ZD".GR1_BT.InPos_FC18,//Position erreicht
           Error                    := "DB_SAFE_ZD".GR1_BT.Error_FC18,
           State                    := "DB_SAFE_ZD".GR1_BT.State_FC18);


      U     "DB_SAFE_ZD".GR1_BT.Done_FB11; //Test erfolgreich beendet
      S     "DB_SAFE_ZD".GR1_BT.HM_BTest_ok; // Zwischenmerker Test Z ok

      U     "DB_SAFE_ZD".GR1_BT.HM_BTest_ok; // Zwischenmerker Test Z ok
      UN    "DB_ACHSE3_Z".E_PLCCtrlAx; // Z-Achse nicht mehr von PLC kontrolliert
      S     "DB_SAFE_ZD".GR1_BT.BTest_Fertig; 
      R     "DB_SAFE_ZD".GR1_BT.HM_BTest_ok; // Zwischenmerker Test Z ok
      R     #O_BT_Laeuft; 

//Fault handling

      U     "M_Ruecksetze_Fehler"; 
      R     #O_BT_Error; 
      R     "DB_SAFE_ZD".GR1_BT.BT_error; 
      R     "DB_SAFE_ZD".GR1_BT.Start_BT_FB11; 
      R     "DB_SAFE_ZD".GR1_BT.Error_FC18; 
      R     #O_BT_Laeuft; 

// Fehler erkannt durch FC 18

      U     "DB_SAFE_ZD".GR1_BT.Error_FC18; 
      =     #O_BT_Error_FC18; 

// Fehler Bremse defekt

      O(    ; 
      L     #T_fnum; 
      L     7; 
      ==I   ; 
      )     ; 
      O(    ; 
      L     #T_fnum; 
      L     8; 
      ==I   ; 
      )     ; 
      U     "DB_SAFE_ZD".GR1_BT.BT_error; 
      S     #O_Bremse_defekt; 

// Fehler erkannt durch FB 11

      U     "DB_SAFE_ZD".GR1_BT.BT_error; 
      R     #O_BT_Laeuft; 

// sonstiger Fehler beim Bremsentest

      U     "DB_SAFE_ZD".GR1_BT.BT_error; 
      UN    #O_Bremse_defekt; 
      S     #O_BT_Error; //-> Sammelmeldung ausgeben 

// Zustandsmeldung Bremsentest laeuft

      U     #O_BT_Laeuft; 
      =     "DB_SAFE_ZD".Seco_Test.Test_Bremse_laeuft; 

NETWORK
TITLE =Test der mechanischen Funktion der Bremse Kopf-Achse

      UN    "O_AL_5ACHS_Bremse_auf"; 
      =     "DB_SAFE_ZD".GR1_BT2.Bclosed_FB11; 

      U     #I_BT_2_Anfordern; //  Anstoss fuer den Bremsentest Z-Achse
      FP    "DB_SAFE_ZD".GR1_BT2.BT_Start_Taste_FP; 
      UN    "DB_SAFE_ZD".GR1_BT2.BT_error; //Fehler aufgetreten
      S     "DB_SAFE_ZD".GR1_BT2.BT_laeuft; //Bremsentest laeuft
      S     "DB_SAFE_ZD".GR1_BT2.Schritt1; //naechster Schritt
      S     "DB_ACHSE5".A_NCASpStrobe; //Neutrale Achse anfordern
      R     "DB_ACHSE5".A_NCASpChanA; //Neutrale Achse anfordern
      S     #O_BT_2_Laeuft; //Bremsentest laeuft

      U     "DB_ACHSE5".E_NeutrASp; //Rueckmeldung Achse ist neutral
      U     "DB_SAFE_ZD".GR1_BT2.Schritt1; 
      FP    "DB_SAFE_ZD".GR1_BT2.Schritt1_FP; 
      S     "DB_SAFE_ZD".GR1_BT2.Schritt2; //naechster Schritt
      R     "DB_SAFE_ZD".GR1_BT2.Schritt1; 
      R     "DB_ACHSE5".A_NCASpStrobe; 
      S     "DB_ACHSE5".A_PLCAxis; //PLC-kontrollierte Achse anfordern (MD 10008)

      U     "DB_ACHSE5".E_PLCCtrlAx; //Rueckmeldung Achse wird von PLC kontrolliert
      U     "DB_SAFE_ZD".GR1_BT2.Schritt2; 
      FP    "DB_SAFE_ZD".GR1_BT2.Schtitt2_FP; 

      R     "DB_SAFE_ZD".GR1_BT2.Schritt2; 
      S     "DB_SAFE_ZD".GR1_BT2.Start_BT_FB11; //Start Bremsentest fuer FB

      CALL "SI_BrakeTest" , "DB_SAFE_BTGR2_INST" (
           Start                    := "DB_SAFE_ZD".GR1_BT2.Start_BT_FB11,
           Quit                     := "M_Ruecksetze_Fehler",//"MMC_SIGNALS".E_Cancel
           Bclosed                  := "DB_SAFE_ZD".GR1_BT2.Bclosed_FB11,
           Axis                     := 5,
           TimerNo                  := "T_SAFECO_Bremsentest_2",
           TV_BTactiv               := S5T#200MS,
           TV_Bclose                := S5T#1S,
           TV_FeedCommand           := S5T#1S,
           TV_FXSreached            := S5T#1S,
           TV_FXShold               := S5T#1S500MS,
           CloseBrake               := "DB_SAFE_ZD".GR1_BT2.Bremse_schliessen,
           MoveAxis                 := "DB_SAFE_ZD".GR1_BT2.MoveAxis_FB11,
           Done                     := "DB_SAFE_ZD".GR1_BT2.Done_FB11,
           Error                    := "DB_SAFE_ZD".GR1_BT2.BT_error,
           State                    := "DB_SAFE_ZD".GR1_BT2.State_FB11);

// S t a t e
// -----------
// 0:Kein Fehler                                                                                                            
// 1:Startbedingungen nicht gegeben, z.B. Achse nicht in Regelung / Bremse zu / Achsensperre steht an.                      
// 2:Bei Anwahl Bremsentest erfolgt keine Rückmeldung der NC im Signal "Bremsentest aktiv"                                  
// 3:Keine Rückmeldung "Bremse eingefallen" durch Eingangssignal BClosed                                                    
// 4:Keine Fahrbefehlausgabe (z.B. Bewegung der Achse wurde nicht gestartet)                                                
// 5:Festanschlag wird nicht erreicht -> Achs-RESET wurde ausgelöst                                                         
// 6:Verfahrsperre/Anfahrt zu langsam -> Festanschlag kann nicht erreicht werden. Überwachungszeit TV_FXSreached abgelaufen.
// 7:Bremse hält überhaupt nicht (Endposition wird erreicht) / Anfahrgeschwindigkeit zu groß                                
// 8:Bremse bricht während der Haltezeit auf                                                                                
// 9:Fehler bei Bremsentest-Abwahl                                                                                          
//10:Interner Fehler                                                                                                        
//11:Signal "PLC kontrolliert Achse" nicht eingeschaltet vom Anwenderprogramm (MD 10008)


      U     "M_Ruecksetze_Fehler"; 
      SPBN  er3; 
      L     0; 
      T     #T_errword; 
      R     "DB_SAFE_ZD".GR1_BT2.BT_error; 
      SPA   equ3; 

er3:  U     "DB_SAFE_ZD".GR1_BT2.BT_error; 
      SPBN  een3; 
      L     "DB_SAFE_ZD".GR1_BT2.State_FB11; 
      T     #T_fnum; 
      L     0; 
      ==I   ; 
      SPB   een3; 
      L     0; 
      T     #T_errword; 
      L     #T_fnum; 
      DEC   1; 
      L     8; 
      MOD   ; 
      T     #T_Bit; 
      L     #T_fnum; 
      L     8; 
      /I    ; 
      T     #T_Byte; 
      SLD   3; 
      L     #T_Bit; 
      OW    ; 
      LAR1  ; 
      L     P##T_errword; 
      +AR1  ; 
      S     L [AR1,P#0.0]; 
equ3: L     #T_errword; 
      AUF   "DB_FEHLERMELDUNG"; 
      L     P#DBX 114.0; 
      LAR1  ; 
      L     #T_errword; 
      T     DBW [AR1,P#0.0]; 
een3: NOP   0; 


// Testmoment wird über MD 36966 definiert / Toleranz im MD 36967
// Beispiel: Haltemoment für Z-Achse 28% (MD 1728)
// Testmoment |50%| -> Achsgewicht (28%) ==> Motor (-23%) : MD 1728

      O     "DB_SAFE_ZD".GR1_BT2.Done_FB11; //Test erfolgreich beendet
      O     "DB_SAFE_ZD".GR1_BT2.BT_error; //Fehler aufgetreten
      FP    "DB_SAFE_ZD".GR1_BT2.Done_Error_FB11_FP; 
      R     "DB_ACHSE5".A_PLCAxis; //Anforderung PLC-kontrollierte Achse rücksetzen

      UN    "DB_ACHSE5".E_PLCCtrlAx; //Rueckmeldung Achse wird von PLC kontrolliert
      U     "DB_SAFE_ZD".GR1_BT2.Start_BT_FB11; //Start Bremsentest fuer FB
      U     "DB_SAFE_ZD".GR1_BT2.BT_laeuft; //Bremsentest laeuft
      FP    "DB_SAFE_ZD".GR1_BT2.BT_laeuft_PLCAx_FP; 
      R     "DB_SAFE_ZD".GR1_BT2.Start_BT_FB11; //Start Bremsentest fuer FB
      R     "DB_SAFE_ZD".GR1_BT2.BT_laeuft; //Bremsentest laeuft

      CALL "FC_SIEM_SPINCTRL" (//Z-Achse verfahren
           Start                    := "DB_SAFE_ZD".GR1_BT2.MoveAxis_FB11,//Start des Verfahrbewegung
           Stop                     := FALSE,
           Funct                    := B#16#5,//Mode: Achsbetrieb
           Mode                     := B#16#1,//Verfahren: Inkrementell
           AxisNo                   := 5,//Achsnummer der zu verfahrenden Achse Z-Achse
           Pos                      := 3.000000e+000,//Verfahrweg: minus 1 Grad
           FRate                    := 2.000000e+003,//Vorschub: 2 mu/min
           InPos                    := "DB_SAFE_ZD".GR1_BT2.InPos_FC18,//Position erreicht
           Error                    := "DB_SAFE_ZD".GR1_BT2.Error_FC18,
           State                    := "DB_SAFE_ZD".GR1_BT2.State_FC18);


      U     "DB_SAFE_ZD".GR1_BT2.Done_FB11; //Test erfolgreich beendet
      S     "DB_SAFE_ZD".GR1_BT2.HM_BTest_ok; // Zwischenmerker Test Z ok

      U     "DB_SAFE_ZD".GR1_BT2.HM_BTest_ok; // Zwischenmerker Test Z ok
      UN    "DB_ACHSE3_Z".E_PLCCtrlAx; // Z-Achse nicht mehr von PLC kontrolliert
      S     "DB_SAFE_ZD".GR1_BT2.BTest_Fertig; 
      R     "DB_SAFE_ZD".GR1_BT2.HM_BTest_ok; // Zwischenmerker Test Z ok
      R     #O_BT_2_Laeuft; 


//Fault handling

      U     "M_Ruecksetze_Fehler"; //DB19.DBX   20.2
      R     #O_BT_2_Error; 
      R     #O_BT_2_Laeuft; 
      R     "DB_SAFE_ZD".GR1_BT2.BT_error; 
      R     "DB_SAFE_ZD".GR1_BT2.Start_BT_FB11; 
      R     "DB_SAFE_ZD".GR1_BT2.Error_FC18; 

// -- Fehler erkannt durch FC18

      U     "DB_SAFE_ZD".GR1_BT2.Error_FC18; 
      =     #O_BT_Error_FC18; 

// -- Fehler Bremse defekt

      O(    ; 
      L     #T_fnum; 
      L     7; 
      ==I   ; 
      )     ; 
      O(    ; 
      L     #T_fnum; 
      L     8; 
      ==I   ; 
      )     ; 
      U     "DB_SAFE_ZD".GR1_BT2.BT_error; 
      S     #O_Bremse_2_defekt; 

// -- Fehler erkannt durch FB11

      U     "DB_SAFE_ZD".GR1_BT2.BT_error; 
      R     #O_BT_2_Laeuft; 

// sonstiger Fehler beim Bremsentest

      U     "DB_SAFE_ZD".GR1_BT2.BT_error; 
      UN    #O_Bremse_2_defekt; 
      S     #O_BT_2_Error; //-> Sammelmeldung ausgeben 

// Zustandsmeldung Bremsentest laeuft

      U     #O_BT_2_Laeuft; 
      =     "DB_SAFE_ZD".Seco_Test.Test_Bremse_laeuft; 

NETWORK
TITLE =Schrittkette: Abschaltpfade testen / externe Stops testen
// *** Schritt 1: Bremse(n) einfallen lassen (wenn Startbedingungen gegeben) ***
      U     #I_ZD_Anfordern; // U E 6.2 Anstoss fuer den Bremsentest Z-Achse
      FP    "DB_SAFE_ZD".GR1_ZD.Start_Taste_FP; 
      S     "DB_SAFE_ZD".GR1_ZD.Schritt1; 
      S     #O_ZD_Laeuft; 

      U     "DB_SAFE_ZD".GR1_ZD.Schritt1; 
      S     "DB_SAFE_ZD".GR1_ZD.Bremse; 

// *** Schritt 2: RF für Z wegnehmen (sonst Arbeiten gegen Bremse) ***

      U     "DB_SAFE_ZD".GR1_ZD.Schritt1; 
      L     S5T#300MS; 
      SE    "T_SAFECO_GR1ZD_Schritt_2"; 

      U     "T_SAFECO_GR1ZD_Schritt_2"; 
      S     "DB_SAFE_ZD".GR1_ZD.Schritt2; 
      R     "DB_SAFE_ZD".GR1_ZD.Schritt1; 
      S     "DB_SAFE_ZD".GR1_ZD.Reglerfreigabe; 

// *** Schritt 3: ARBEITSRAUMACHSEN Test der Abschaltpfade (Antrieb/NCK intern) ***

      U     "DB_SAFE_ZD".GR1_ZD.Schritt2; 
      S     "DB_SAFE_ZD".GR1_ZD.Schritt3; 
      R     "DB_SAFE_ZD".GR1_ZD.Schritt2; 
      S     "DB_SAFE_ZD".Seco_Test.Test_ABSP_laeuft; 

      CALL "FC_SPL_ZD1_6AX1" (
           start                    := "DB_SAFE_ZD".GR1_ZD.Schritt3,
           clear                    := "M_Reset_Taste",
           num_axis                 := #T_ANZAHL_ARB_ACHSEN,
           test_axis_1              := 1,//X
           test_axis_2              := 2,//Z oder Y
           test_axis_3              := 3,//Y oder Z
           test_axis_4              := 4,//B oder C
           test_axis_5              := 6,//SPI
           test_axis_6              := #T_ARB_ACHSE_6_NUMMER,//C oder Schwenkrundtisch oder Teilapperat
           aux_dword                := "DB_SAFE_ZD".GR1_ZD.aux_dword_ZD1_int1,
           ready                    := #S3_READY,
           serror                   := #FC_SPL_ZD1_SERROR1,// Startbed. nicht gegeben : mind. 1 Achse mit DBX108.2 == 0
           error                    := #FC_SPL_ZD1_ERROR1);// Parametrierfehler Achsanzahl ...

// *** Schritt 4: ZUSATZACHSEN Test der Abschaltpfade (Antrieb/NCK intern) ***

      UN    "DB_SAFE_ZD".GR1_ZD.Schritt3; 
      U     "DB_SAFE_ZD".GR1_ZD.Schritt4; 
      =     #S4_START; 

      U     #S3_READY; 
      S     "DB_SAFE_ZD".GR1_ZD.Schritt4; 
      R     "DB_SAFE_ZD".GR1_ZD.Schritt3; 

      CALL "FC_SPL_ZD1_6AX1" (
           start                    := #S4_START,
           clear                    := "M_Reset_Taste",//"MMC_SIGNALS".E_Cancel
           num_axis                 := #T_ANZAHL_ZUSATZ_ACHSEN,
           test_axis_1              := #T_ZUSATZ_ACHSE_1_NUMMER,// MAG
           test_axis_2              := #T_ZUSATZ_ACHSE_2_NUMMER,
           test_axis_3              := #T_ZUSATZ_ACHSE_3_NUMMER,
           test_axis_4              := #T_ZUSATZ_ACHSE_4_NUMMER,
           test_axis_5              := #T_ZUSATZ_ACHSE_5_NUMMER,
           test_axis_6              := #T_ZUSATZ_ACHSE_6_NUMMER,
           aux_dword                := "DB_SAFE_ZD".GR1_ZD.aux_dword_ZD1_int2,
           ready                    := "DB_SAFE_ZD".GR1_ZD.Schritt5,
           serror                   := #FC_SPL_ZD1_SERROR2,// Startbed. nicht gegeben : mind. 1 Achse mit DBX108.2 == 0
           error                    := #FC_SPL_ZD1_ERROR2);// Parametrierfehler Achsanzahl ...

// *** Schritt 5: ARBEITSRAUMACHSEN Test der externen Stopps (STOP D -> STOP C -> STOP A) : [1] Antrieb / [2] NCK ***

      U     "DB_SAFE_ZD".GR1_ZD.Schritt5; 
      R     "DB_SAFE_ZD".Seco_Test.Test_ABSP_laeuft; 
      S     "DB_SAFE_ZD".Seco_Test.Test_extStop_laeuft; 
      R     "DB_SAFE_ZD".GR1_ZD.Schritt4; 

      CALL "FC_SPL_ZD2_8AX1" (
           start                    := "DB_SAFE_ZD".GR1_ZD.Schritt5,
           clear                    := "M_Reset_Taste",//"MMC_SIGNALS".E_Cancel
           num_axis                 := #T_ANZAHL_ARB_ACHSEN,
           test_axis_1              := 1,//X
           test_axis_2              := 2,//Z oder Y
           test_axis_3              := 3,//Y oder Z
           test_axis_4              := 4,//B oder C
           test_axis_5              := 6,//SPI
           test_axis_6              := #T_ARB_ACHSE_6_NUMMER,//C oder Schwenkrundtisch oder Teilapperat
           test_axis_7              := 0,
           test_axis_8              := 0,
           req_stopd_plc            := "DB_SAFE_ZD".GR1_ZD.req_stopd_plc,
           req_stopc_plc            := "DB_SAFE_ZD".GR1_ZD.req_stopc_plc,
           req_stopa_plc            := "DB_SAFE_ZD".GR1_ZD.req_stopa_plc,
           req_stopd_nck            := "DB_SPL".IPLC_ZD_Gr1_STOPD,
           req_stopc_nck            := "DB_SPL".IPLC_ZD_Gr1_STOPC,
           req_stopa_nck            := "DB_SPL".IPLC_ZD_Gr1_STOPA,
           aux_dword                := "DB_SAFE_ZD".GR1_ZD.aux_dword_ZD2_ext1,
           ready                    := #S5_READY,
           serror                   := #FC_SPL_ZD2_SERROR1,
           error                    := #FC_SPL_ZD2_ERROR1);

// *** Schritt 6: ZUSATZACHSEN Test der externen Stopps (STOP D -> STOP C -> STOP A) : [1] Antrieb / [2] NCK ***

      UN    "DB_SAFE_ZD".GR1_ZD.Schritt5; 
      U     "DB_SAFE_ZD".GR1_ZD.Schritt6; 
      =     #S6_START; 

      U     #S5_READY; 
      S     "DB_SAFE_ZD".GR1_ZD.Schritt6; 
      R     "DB_SAFE_ZD".GR1_ZD.Schritt5; 

      CALL "FC_SPL_ZD2_8AX1" (
           start                    := #S6_START,
           clear                    := "M_Reset_Taste",//"MMC_SIGNALS".E_Cancel
           num_axis                 := #T_ANZAHL_ZUSATZ_ACHSEN,
           test_axis_1              := #T_ZUSATZ_ACHSE_1_NUMMER,
           test_axis_2              := #T_ZUSATZ_ACHSE_2_NUMMER,
           test_axis_3              := #T_ZUSATZ_ACHSE_3_NUMMER,
           test_axis_4              := #T_ZUSATZ_ACHSE_4_NUMMER,
           test_axis_5              := #T_ZUSATZ_ACHSE_5_NUMMER,
           test_axis_6              := #T_ZUSATZ_ACHSE_6_NUMMER,
           test_axis_7              := 0,
           test_axis_8              := 0,
           req_stopd_plc            := "DB_SAFE_ZD".GR1_ZD.req_stopd_plc,
           req_stopc_plc            := "DB_SAFE_ZD".GR1_ZD.req_stopc_plc,
           req_stopa_plc            := "DB_SAFE_ZD".GR1_ZD.req_stopa_plc,
           req_stopd_nck            := "DB_SPL".IPLC_ZD_Gr1_STOPD,
           req_stopc_nck            := "DB_SPL".IPLC_ZD_Gr1_STOPC,
           req_stopa_nck            := "DB_SPL".IPLC_ZD_Gr1_STOPA,
           aux_dword                := "DB_SAFE_ZD".GR1_ZD.aux_dword_ZD2_ext2,
           ready                    := "DB_SAFE_ZD".GR1_ZD.Schritt7,
           serror                   := #FC_SPL_ZD2_SERROR2,
           error                    := #FC_SPL_ZD2_ERROR2);

// *** Schritt 7: Abschluss --> RF wieder setzen / Bremse wieder freigeben ***

      U     "DB_SAFE_ZD".GR1_ZD.Schritt7; 
      R     "DB_SAFE_ZD".GR1_ZD.Schritt6; 
      R     "DB_SAFE_ZD".GR1_ZD.Reglerfreigabe; // RF Z wieder geben
      R     "DB_SAFE_ZD".Seco_Test.Test_extStop_laeuft; 
      R     "DB_SAFE_ZD".GR1_ZD.Bremse; // Bremse Z schliessen

// *** Schritt 8: Fertigmeldung ***

      U     "DB_SAFE_ZD".GR1_ZD.ZD_Fertig; 
      R     "DB_SAFE_ZD".GR1_ZD.Schritt8; 
      R     "DB_SAFE_ZD".GR1_ZD.ZD_Fertig; 
      R     #O_ZD_Laeuft; 

      U     "DB_SAFE_ZD".GR1_ZD.Schritt8; 
      S     "DB_SAFE_ZD".GR1_ZD.ZD_Fertig; 

      U     "DB_SAFE_ZD".GR1_ZD.Schritt7; 
      S     "DB_SAFE_ZD".GR1_ZD.Schritt8; 
NETWORK
TITLE =Sammelmeldung Test der Abschaltpfade error

      O     #FC_SPL_ZD1_ERROR1; 
      O     #FC_SPL_ZD1_ERROR2; 
      =     "DB_SAFE_ZD".GR1_ZD.ZD1_error_int; 
NETWORK
TITLE =Sammelmeldung Test der Abschaltpfade serror

      O     #FC_SPL_ZD1_SERROR1; 
      O     #FC_SPL_ZD1_SERROR2; 
