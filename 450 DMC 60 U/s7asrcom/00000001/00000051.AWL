NETWORK
TITLE =Sammelmeldung Test der externen Stops error

      O     #FC_SPL_ZD2_ERROR1; 
      O     #FC_SPL_ZD2_ERROR2; 
      =     "DB_SAFE_ZD".GR1_ZD.ZD2_serror; 
NETWORK
TITLE =Sammelmeldung Test der externen Stops serror

      O     #FC_SPL_ZD2_SERROR1; 
      O     #FC_SPL_ZD2_SERROR2; 
      =     "DB_SAFE_ZD".GR1_ZD.ZD2_error; 
NETWORK
TITLE =Sammelmeldung ausgeben 

      O     "DB_SAFE_ZD".GR1_ZD.ZD1_serror_int; 
      O     "DB_SAFE_ZD".GR1_ZD.ZD2_serror; 
      S     #O_ZD_SError; 
NETWORK
TITLE =Fehler löschen / Reset

      UN    "M_Ruecksetze_Fehler"; 
      SPB   ndel; 

// HW-Test
      R     "DB_SAFE_ZD".Seco_Test.Test_HW_laueft; 
      R     "DB_SAFE_ZD".Seco_Test.Test_Bremse_laeuft; 

// Bremsentest
      R     "DB_SAFE_ZD".GR1_BT.BT_laeuft; //Bremsentest laeuft
      R     "DB_SAFE_ZD".GR1_ZD.Bremse; // Bremse Z schliessen
      R     #O_BT_Laeuft; 
      R     #O_BT_2_Laeuft; 

// Abschaltpfade / externe Stops
      R     #O_ZD_Laeuft; 
      L     0; 
      T     DB397.DBW    0; 
      T     "DB_SAFE_ZD".GR1_ZD.aux_dword_ZD1_int1; 
      T     "DB_SAFE_ZD".GR1_ZD.aux_dword_ZD1_int2; 
      T     "DB_SAFE_ZD".GR1_ZD.aux_dword_ZD2_ext1; 
      T     "DB_SAFE_ZD".GR1_ZD.aux_dword_ZD2_ext2; 
      R     "DB_SPL".IPLC_ZD_Gr1_INT; 
      R     "DB_SAFE_ZD".Seco_Test.Test_ABSP_laeuft; 
      R     "DB_SAFE_ZD".Seco_Test.Test_extStop_laeuft; 

      R     #O_ZD_SError; 
      R     "DB_SAFE_ZD".GR1_ZD.Reglerfreigabe; // RF Z wieder geben
      R     "DB_SAFE_ALLG1".M_Bremse_Z_schliessen; //"M_Bremse_Z_schliessen"; // Bremse Z schliessen

ndel: NOP   0; 


