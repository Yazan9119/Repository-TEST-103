      U     "DB_WZV".AB_HSK; 
      =     #t_Kegelreinigung; 

NETWORK
TITLE =Anwahl WZW_MASKE_3: WZW-Reinigung und Verriegelung

      UN    "M_WZM_Regal_angewaehlt"; 
      U     "DB_CONFIG".ANWAHL_DURCH_USER_DATA.WZW_HORIZONTALE_KETTE; 
      O     ; 
      U(    ; 
      O     "DB_CONFIG".ANWAHL_DURCH_USER_DATA.WZW_VKETTE_MIT_TPU; 
      O     "DB_CONFIG".ANWAHL_DURCH_USER_DATA.WZW_VKETTE_OHNE_TPU; 
      )     ; 
      U     "DB_PLC_MD_DB20".UDHex._04_BIT4_EINRICHT_BETR; 
      O     #t_Kegelreinigung; 
      =     "DB_OEM".Anwahl.WZW.Maske3; 
NETWORK
TITLE =Anwahl WZW_MASKE_4: WZW-Regalrettung

      U     "M_WZM_Regal_angewaehlt"; 
      =     "DB_OEM".Anwahl.WZW.Maske4; 
NETWORK
