VERSION : 0.1


VAR_TEMP
  t_VSK_Ausblenden_Zeile : ARRAY  [1 .. 8 ] OF BOOL ;	
  t_ARTIS_angewaehlt : BOOL ;	
  t_MCU_angewaehlt : BOOL ;	
  t_Wechsler_V_Kette_m_TPU : BOOL ;	
  t_Wechsler_V_Kette_o_TPU : BOOL ;	
