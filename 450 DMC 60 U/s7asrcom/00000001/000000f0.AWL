//  - Handlingsystem SYSTEM3R mit Beladetür (Anwahl: 1=ohne RPC, 2=mit RPC)
//  - Handlingsystem EROWA, PROMOT mit Beladetür (Anwahl: 5=ohne RPC, 2=mit RPC)
//
// Alle Zeilen Ausblenden und je nach Anwahl wieder einblenden
      L     B#16#3F; 
      T     "DB_OEM".RowInaktiv.Schwenkkopf.Maske1; 
      L     B#16#3F; 
      T     "DB_OEM".RowInaktiv.Schwenkkopf.Maske6; 
      L     B#16#3F; 
      T     "DB_OEM".RowInaktiv.Schwenkkopf.Maske7; 

      CLR   ; 
      =     "DB_OEM".Anwahl.Option.SK; 
      =     "DB_OEM".Anwahl.Option.System3R; 

      L     "DB_PLC_MD_DB20".UDInt._007_ANWAHL_FRAESKOPF; 
      L     1; 
      ==I   ; 
      S     "DB_OEM".Anwahl.Option.SK; 
      R     DB101.DBX   58.0; 
      R     DB101.DBX   58.1; 


      U     "DB_CONFIG".ANWAHL_DURCH_USER_DATA.HASYS_3R_OHNE_RPC; 
      O     "DB_CONFIG".ANWAHL_DURCH_USER_DATA.HASYS_EROWA_OHNE_RPC; 
      S     "DB_OEM".Anwahl.Option.System3R; 
      R     DB101.DBX   63.0; 
      R     DB101.DBX   63.4; 
      R     DB101.DBX   63.5; 
      R     DB101.DBX   64.0; 
      R     DB101.DBX   64.1; 


      U     "DB_CONFIG".ANWAHL_DURCH_USER_DATA.HASYS_3R_MIT_RPC; 
      O     "DB_CONFIG".ANWAHL_DURCH_USER_DATA.HASYS_EROWA_MIT_RPC; 
      S     "DB_OEM".Anwahl.Option.System3R; 
      R     DB101.DBX   63.0; 
      R     DB101.DBX   63.1; 
      R     DB101.DBX   63.2; 
      R     DB101.DBX   63.3; 
      R     DB101.DBX   63.4; 
      R     DB101.DBX   63.5; 
      R     DB101.DBX   64.0; 
      R     DB101.DBX   64.1; 


NETWORK
TITLE =Anwahl Option: WFK1 (Winkelfräskopf)
//      U     "DB_PLC_MD_DB20".UDHex._28_Bit3_WFK_Anwahl
//      =     "DB_OEM".Anwahl.Option.WFK1
NETWORK
TITLE =Anwahl Option: WFK2 (Winkelfräskopf)
//      U     "DB_PLC_MD_DB20".UDHex._28_Bit3_WFK_Anwahl
//      =     "DB_OEM".Anwahl.Option.WFK2
NETWORK
TITLE =Anwahl Option: Haupt-Ebene

      O     "DB_OEM".Anwahl.Option.SK; 
//      O     "DB_OEM".Anwahl.Option.WFK1
//      O     "DB_OEM".Anwahl.Option.WFK2
      O     "DB_OEM".Anwahl.Option.System3R; 
      =     "DB_OEM".Anwahl.Option.Ebene; 
NETWORK
TITLE =Anwahl Blum_Maske_1: WZ-Vermessung Einstiegsmaske

      O(    ; 
      L     "DB_PLC_MD_DB20".UDInt._032_ANWAHL_WZVERMESSUNG; 
      L     0; 
      <>I   ; 
      )     ; 
      O(    ; 
      L     "DB_PLC_MD_DB20".UDReal._16_ANWAHL_KALIBRIERRING; 
      L     1.000000e+000; 
      ==R   ; 
      )     ; 
      =     "DB_OEM".Anwahl.Blum.Maske1; 
NETWORK
TITLE =Anwahl Blum_Maske_2: WZ-Vermessung

      U(    ; 
      L     "DB_PLC_MD_DB20".UDInt._032_ANWAHL_WZVERMESSUNG; 
      L     1; 
      ==I   ; 
      TAK   ; 
      O(    ; 
      L     2; 
      ==I   ; 
      )     ; 
      TAK   ; 
      O(    ; 
      L     3; 
      ==I   ; 
      )     ; 
      TAK   ; 
      O(    ; 
      L     4; 
      ==I   ; 
      )     ; 
      TAK   ; 
      O(    ; 
      L     5; 
      ==I   ; 
      )     ; 
      TAK   ; 
      O(    ; 
      L     7; 
      ==I   ; 
      )     ; 
      )     ; 
      =     "DB_OEM".Anwahl.Blum.Maske2; 
NETWORK
TITLE =Anwahl Blum_Maske_3: Kalibrierring

      L     "DB_PLC_MD_DB20".UDReal._16_ANWAHL_KALIBRIERRING; 
      L     1.000000e+000; 
      ==R   ; 
      =     "DB_OEM".Anwahl.Blum.Maske3; 
NETWORK
TITLE =Adressregister fuer Zeilen ausblenden

      LAR1  P##t_VSK_Ausblenden_Zeile; 

NETWORK
TITLE =Abwahl Zeilen für Blum-Maske 1

      UN    "DB_OEM".Anwahl.Blum.Maske1; 
      =     #t_VSK_Ausblenden_Zeile[1]; 

      UN    "DB_OEM".Anwahl.Blum.Maske1; 
      =     #t_VSK_Ausblenden_Zeile[2]; 

      UN    "DB_OEM".Anwahl.Blum.Maske1; 
      =     #t_VSK_Ausblenden_Zeile[3]; 

      UN    "DB_OEM".Anwahl.Blum.Maske1; 
      =     #t_VSK_Ausblenden_Zeile[4]; 

      UN    "DB_OEM".Anwahl.Blum.Maske1; 
      =     #t_VSK_Ausblenden_Zeile[5]; 

      UN    "DB_OEM".Anwahl.Blum.Maske1; 
      =     #t_VSK_Ausblenden_Zeile[6]; 

// Zeile 7+8 müssen immer mit CLR angesteuert werden
      CLR   ; 
      =     #t_VSK_Ausblenden_Zeile[7]; 
      =     #t_VSK_Ausblenden_Zeile[8]; 

//Übergabe in DB
      L     B [AR1,P#0.0]; 
      T     "DB_OEM".RowInaktiv.BLUM.Maske1; 
NETWORK
TITLE =Anwahl PW_MASKE_1: PW1
// Palettenwechsler Maske 1 und 2 und 3
      L     "DB_PLC_MD_DB20".UDInt._015_ANWAHL_PALWECHLSER; 
      L     0; 
      <>I   ; 
      =     "DB_OEM".Anwahl.PW.Maske1; 
NETWORK
TITLE =Anwahl PW_MASKE_2: PW2
// Palettenwechsler Maske 1 und 2 und 3
      L     "DB_PLC_MD_DB20".UDInt._015_ANWAHL_PALWECHLSER; 
      L     0; 
      <>I   ; 
      =     "DB_OEM".Anwahl.PW.Maske2; 
NETWORK
TITLE =Anwahl PW_MASKE_3: PW3
// Palettenwechsler Maske 1 und 2 und 3
      U(    ; 
      L     "DB_PLC_MD_DB20".UDInt._015_ANWAHL_PALWECHLSER; 
      L     0; 
      <>I   ; 
      )     ; 
      U(    ; 
      O     "DB_HR".ABST.SP_MPL_Kuppeln; 
      O     "DB_PW_DATA".Option.bRP_Hydr_Klemmung; 
      O     "DB_HR".ABST.SpannHydraulik; 
      O     "DB_PW_DATA".Option2.bMaschineTypFD_70_80; 
      )     ; 
      =     "DB_OEM".Anwahl.PW.Maske3; 
NETWORK
TITLE =Anwahl Speicher_Ebene:

      O     "DB_CONFIG".ANWAHL_DURCH_USER_DATA.RS4_AKTIV; 
      O     "DB_CONFIG".ANWAHL_DURCH_USER_DATA.RS5_AKTIV; 
      O     "DB_CONFIG".ANWAHL_DURCH_USER_DATA.RS10_AKTIV; 
      O(    ; 
      L     "DB_PLC_MD_DB20".UDInt._134_PS_SONDERFUNKTION; 
      L     1; 
      ==I   ; 
      )     ; 
      =     "DB_OEM".Anwahl.Speicher.Ebene; 
NETWORK
TITLE =Anwahl Speicher_Maske_1:

      O     "DB_CONFIG".ANWAHL_DURCH_USER_DATA.RS4_AKTIV; 
      O     "DB_CONFIG".ANWAHL_DURCH_USER_DATA.RS5_AKTIV; 
      O     "DB_CONFIG".ANWAHL_DURCH_USER_DATA.RS10_AKTIV; 
      =     "DB_OEM".Anwahl.Speicher.Maske1; 
NETWORK
TITLE =Anwahl Speicher_Maske_2:

      O     "DB_CONFIG".ANWAHL_DURCH_USER_DATA.RS4_AKTIV; 
      O     "DB_CONFIG".ANWAHL_DURCH_USER_DATA.RS5_AKTIV; 
      O     "DB_CONFIG".ANWAHL_DURCH_USER_DATA.RS10_AKTIV; 
      =     "DB_OEM".Anwahl.Speicher.Maske2; 
NETWORK
TITLE =Anwahl Speicher_Maske_3:

      U     "DB_CONFIG".ANWAHL_DURCH_USER_DATA.RS10_AKTIV; 
      =     "DB_OEM".Anwahl.Speicher.Maske3; 
NETWORK
TITLE =Abwahl Zeilen im Bild Speicher 1

      U     "DB_CONFIG".ANWAHL_DURCH_USER_DATA.RS4_AKTIV; 
      =     #t_VSK_Ausblenden_Zeile[1]; 

      CLR   ; 
      =     #t_VSK_Ausblenden_Zeile[2]; 

      U     "DB_CONFIG".ANWAHL_DURCH_USER_DATA.RS5_AKTIV; 
      O     "DB_CONFIG".ANWAHL_DURCH_USER_DATA.RS10_AKTIV; 
      =     #t_VSK_Ausblenden_Zeile[3]; 
      =     #t_VSK_Ausblenden_Zeile[4]; 

      CLR   ; 
      =     #t_VSK_Ausblenden_Zeile[5]; 

      U     "DB_CONFIG".ANWAHL_DURCH_USER_DATA.RS10_AKTIV; 
      =     #t_VSK_Ausblenden_Zeile[6]; 

// Zeile 7+8 müssen immer mit CLR angesteuert werden
      CLR   ; 
      =     #t_VSK_Ausblenden_Zeile[7]; 
      =     #t_VSK_Ausblenden_Zeile[8]; 

//Übergabe in DB
      L     B [AR1,P#0.0]; 
      T     "DB_OEM".RowInaktiv.Speicher.Maske1; 
NETWORK
TITLE =Abwahl Zeilen im Bild Speicher 2

      CLR   ; 
      =     #t_VSK_Ausblenden_Zeile[1]; 

      CLR   ; 
      =     #t_VSK_Ausblenden_Zeile[2]; 

      UN    "DB_PLC_MD_DB20".UDHex._17_BIT7_RP_HYD_KLEMMEN; 
      =     #t_VSK_Ausblenden_Zeile[3]; 

      UN    "DB_PLC_MD_DB20".UDHex._27_Bit3_RPLmitKupplung; 
      =     #t_VSK_Ausblenden_Zeile[4]; 

      CLR   ; 
      =     #t_VSK_Ausblenden_Zeile[5]; 

      CLR   ; 
      =     #t_VSK_Ausblenden_Zeile[6]; 

// Zeile 7+8 müssen immer mit CLR angesteuert werden
      CLR   ; 
      =     #t_VSK_Ausblenden_Zeile[7]; 
      =     #t_VSK_Ausblenden_Zeile[8]; 

//Übergabe in DB
      L     B [AR1,P#0.0]; 
      T     "DB_OEM".RowInaktiv.Speicher.Maske2; 
NETWORK
TITLE =Abwahl Zeilen im Bild Speicher 3

      CLR   ; 
      =     #t_VSK_Ausblenden_Zeile[1]; 

      CLR   ; 
      =     #t_VSK_Ausblenden_Zeile[2]; 

      CLR   ; 
      =     #t_VSK_Ausblenden_Zeile[3]; 

      CLR   ; 
      =     #t_VSK_Ausblenden_Zeile[4]; 

      CLR   ; 
      =     #t_VSK_Ausblenden_Zeile[5]; 

      CLR   ; 
      =     #t_VSK_Ausblenden_Zeile[6]; 

// Zeile 7+8 müssen immer mit CLR angesteuert werden
      CLR   ; 
      =     #t_VSK_Ausblenden_Zeile[7]; 
      =     #t_VSK_Ausblenden_Zeile[8]; 

//Übergabe in DB
      L     B [AR1,P#0.0]; 
      T     "DB_OEM".RowInaktiv.Speicher.Maske3; 
NETWORK
TITLE =Anwahl Speicher_Maske_4: Wiegenfräskopf

      L     "DB_PLC_MD_DB20".UDInt._134_PS_SONDERFUNKTION; 
      L     1; 
      ==I   ; 
      =     "DB_OEM".Anwahl.Speicher.Maske4; 
NETWORK
TITLE =Anwahl Speicher_Maske_5: Wiegenfräskopf 2

      L     "DB_PLC_MD_DB20".UDInt._134_PS_SONDERFUNKTION; 
      L     1; 
      ==I   ; 
      =     "DB_OEM".Anwahl.Speicher.Maske5; 
NETWORK
TITLE =#t_Wechsler_VKette_m_TPU (mit TPU)

      U     "DB_CONFIG".ANWAHL_DURCH_USER_DATA.WZW_VKETTE_MIT_TPU; 
      =     #t_Wechsler_V_Kette_m_TPU; 

NETWORK
TITLE =#t_Wechsler_VKette_o_TPU (ohne TPU)

      U     "DB_CONFIG".ANWAHL_DURCH_USER_DATA.WZW_VKETTE_OHNE_TPU; 
      =     #t_Wechsler_V_Kette_o_TPU; 

NETWORK
TITLE =#t_Wechsler_Rad

      U     "DB_CONFIG".ANWAHL_DURCH_USER_DATA.WZW_RAD_MIT_ZYLINDER; 
      =     #t_Wechsler_Rad; 

NETWORK
TITLE =#t_Wechsler_H_Magazin

      U     "DB_CONFIG".ANWAHL_DURCH_USER_DATA.WZW_HORIZONTALE_KETTE; 
      =     #t_Wechsler_H_Magazin; 

NETWORK
TITLE =Abwahl Zeilen für WZW_Maske_1: WZW1

      ON    #t_Wechsler_Rad; 
      O     "DB_PLC_MD_DB20".UDHex._31_Bit1_SOLUTIONLINE; 
      =     #t_VSK_Ausblenden_Zeile[1]; 

      U     #t_Wechsler_V_Kette_m_TPU; 
      =     #t_VSK_Ausblenden_Zeile[2]; 

      UN    #t_Wechsler_V_Kette_o_TPU; 
      =     #t_VSK_Ausblenden_Zeile[3]; 

      CLR   ; 
      =     #t_VSK_Ausblenden_Zeile[4]; 

      CLR   ; 
      =     #t_VSK_Ausblenden_Zeile[5]; 

      O     #t_Wechsler_Rad; 
      =     #t_VSK_Ausblenden_Zeile[6]; 

// Zeile 7+8 müssen immer mit CLR angesteuert werden
      CLR   ; 
      =     #t_VSK_Ausblenden_Zeile[7]; 
      =     #t_VSK_Ausblenden_Zeile[8]; 

//Übergabe in DB
      L     B [AR1,P#0.0]; 
      T     "DB_OEM".RowInaktiv.WZW.Maske1; 
NETWORK
TITLE =Abwahl Zeilen für WZW_Maske_2: WZW2

      CLR   ; 
      =     #t_VSK_Ausblenden_Zeile[1]; 

      CLR   ; 
      =     #t_VSK_Ausblenden_Zeile[2]; 

      CLR   ; 
      =     #t_VSK_Ausblenden_Zeile[3]; 

      CLR   ; 
      =     #t_VSK_Ausblenden_Zeile[4]; 

      U     #t_Wechsler_Rad; 
      =     #t_VSK_Ausblenden_Zeile[5]; 

      U     #t_Wechsler_Rad; 
      =     #t_VSK_Ausblenden_Zeile[6]; 

// Zeile 7+8 müssen immer mit CLR angesteuert werden
      CLR   ; 
      =     #t_VSK_Ausblenden_Zeile[7]; 
      =     #t_VSK_Ausblenden_Zeile[8]; 

//Übergabe in DB
      L     B [AR1,P#0.0]; 
      T     "DB_OEM".RowInaktiv.WZW.Maske2; 
NETWORK
TITLE =Abwahl Zeilen für WZW_Maske_3: WZW3

      UN    #t_Wechsler_V_Kette_m_TPU; 
      UN    #t_Wechsler_V_Kette_o_TPU; 
      ON    #t_Kegelreinigung; 
      =     #t_VSK_Ausblenden_Zeile[1]; 

