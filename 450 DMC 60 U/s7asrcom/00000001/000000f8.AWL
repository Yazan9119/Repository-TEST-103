// Schnellabschaltung Werkzeugbruch 
// Es muss von der Hardwarekonstruktion immer das EB41 verwendet werden
      L     PEB   41; 
      T     "MB_Werkzeugbruch"; //MB    500;BYTE;

      U     "IX_TLB_BIT_0"; //M     500.0;BOOL;M-EINGANG: WZB Binärcodiert
      =     "I_WZB_Bit_0"; //M     339.2;BOOL;M-EINGANG: Werkzeugbruchüberwachung BCD-Zahl 1

      U     "IX_TLB_BIT_1"; //M     500.1;BOOL;M-EINGANG: WZB Binärcodiert
      =     "I_WZB_Bit_1"; //M     339.3;BOOL;M-EINGANG: Werkzeugbruchüberwachung BCD-Zahl 2

      U     "IX_TLB_BIT_2"; //M     500.2;BOOL;M-EINGANG: WZB Binärcodiert
      =     "I_WZB_Bit_2"; //M     339.4;BOOL;M-EINGANG: Werkzeugbruchüberwachung BCD-Zahl 4

      U     "IX_TLB_BIT_3"; //M     500.3;BOOL;M-EINGANG: WZB Binärcodiert
      =     "I_WZB_Bit_3"; //M     339.5;BOOL;M-EINGANG: Werkzeugbruchüberwachung BCD-Zahl 8

      U     "IX_TLB_MEASURING_END"; //M     500.4;BOOL;M-EINGANG: WZB Messung Ende
      =     "I_WZB_Messung_Ende"; //M     339.1;BOOL;M-EINGANG: Werkzeugbruchüberwachung Messwertübernahme

      U     "IX_TLB_HOME_POS"; //M     500.5;BOOL;M-EINGANG: WZB Parkstellung
      =     "I_WZB_Parkstellung"; //M     268.1;BOOL;M-EINGANG: Bruchüberwachung in Parkstellung



// Schnellabschaltung Palettenwechsler mitte 
// Es muss von der Hardwarekobstruktion immer der E66.2 für "IX_PC_LIFT_MID" verwendet werden
// Ist "IX_PC_LIFT_MID" nicht in der Zuli vorhanden ist Temp8 immer null und der "I_PW_Hub_in_Mitte" wird nicht geschrieben 
M018: U     "m_null"; 
      =     #TEMP8; 

      U     "IX_PC_LIFT_MID"; 
      ON    "IX_PC_LIFT_MID"; 
      =     #TEMP8; 


      U     #TEMP8; 
      SPBN  kpcm; 

      L     PEB   66; 
      T     "MB_PERIPHERIE_E_BYTE66"; //MB   2066;BYTE;Zwischenspeicher Peripherie-Eingangsbyte 66

      O     "M_PC_LIFT_MID_PERIPH_E"; //M    2066.2;BOOL;Schneller Eingang Palettenwechsler mitte E66.2 Peripherie-Eingang
      =     "I_PW_Hub_in_Mitte"; //M      16.3;BOOL;M-EINGANG: Palettenwechsler Hub Pos. Mitte
kpcm: NOP   0; 

//Bei H-Maschinen werdem die Spanner-Eingänge durch den OB35 Aufruf gelesen (Span_zu_Span_Zeit Optimierung)
      UN    "DB_CONFIG".ANWAHL_DURCH_USER_DATA.MASCHTYP_3071_AKTIV; 
      SPB   Hma3; 

//für eine weitere Optimierung der "Span zu Span Zeit" bei H-Maschinen mit Solutionline (55H/65H/75H und künftige) 
//werden die Eingänge und Ausgänge des Werkzeugspanners in Zukunft über die entsprechenden Peripherie Ein/Ausgangsbytes 
//im OB35 alle 10ms gelesen bzw. geschreiben.    
//Dies setzt voraus das die E/A Belegung wie unten auch zukünftig bestand hat.
//IX_SPI_TL_CLP_RELEASED    E      63.4   Werkzeugspanner geloest / C2-03.2
//IX_SPI_TL_CLP_CLAMPED    E      63.5  Werkzeugspanner gespannt / C2-03.5
//IX_TOOL_CLAMP_CLPD_WO_TL      E63.7      Werkzeugspanner gespannt ohne Werkzeug


      L     PEB   63; 
      T     "MB_PERIPHERIE_E_BYTE63"; 

      U     "M_WZ_SPA_GEL_PERIPH_E"; // "IX_SPI_TL_CLP_RELEASED"
      =     "I_WZ_SPA_geloest"; 

      U     "M_WZ_SPA_GES_PERIPH_E"; //"IX_SPI_TL_CLP_CLAMPED"
      =     "I_WZ-Spanner_Gesp_O_WZ"; 

      U     "M_WZ_SPA_GES_PERIPH_E"; // "IX_SPI_TL_CLP_CLAMPED"
      =     "I_WZ_SPA_gespannt"; 

//Abfrage ob Schalter"IX_TOOL_CLAMP_CLPD_WO_TL" vorhanden ist
      U     "m_null"; 
      =     #TEMP9; 

      U     "IX_TOOL_CLAMP_CLPD_WO_TL"; 
      ON    "IX_TOOL_CLAMP_CLPD_WO_TL"; 
      =     #TEMP9; 

      U     #TEMP9; 
      SPBN  Hma3; 


      U     "M_WZ_SPA_GES_OW_PERIPH_E"; //E ;BOOL;
      =     "I_WZ-Spanner_Gesp_O_WZ"; //M     338.3;BOOL;M-EINGANG: Werkzeugspanner gespannt ohne Werkzeug

      UN    "M_WZ_SPA_GES_OW_PERIPH_E"; //E ;BOOL;
      U     "M_WZ_SPA_GES_PERIPH_E"; //E ;BOOL;Werkzeugspanner gespannt
      =     "I_WZ-Spanner_Gesp_mit_WZ"; //M     307.0;BOOL;

Hma3: NOP   0; 
NETWORK
TITLE =Baustein-Ende

      BEA   ; 

