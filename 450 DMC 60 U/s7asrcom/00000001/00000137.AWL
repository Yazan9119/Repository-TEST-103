TITLE =Auswertung Status FC8

      UN    #Start; 
      SPB   ENDE; // FC 8 aktiv ? Nein ->
      UN    #Ready; 
      SPB   ERRO; // Ready FC 8 = 1 ? Nein ->
      R     #Start; 
      L     #Status; 
      L     3; 
      ==I   ; 
      SPB   ENDE; // bei Status 3 kein Loeschen
      SPA   LOE; 
ERRO: L     0; 
