      S     #Start_Diagnose; 

FC8:  CALL "FC_SIEM_TM_TRANS" (
           Start                    := #Start,
           TaskIdent                := #TaskIdent,
           TaskIdentNo              := #TaskIdentNo,
           NewToolMag               := #NewToolMag,
           NewToolLoc               := #NewToolLoc,
           OldToolMag               := #OldToolMag,
           OldToolLoc               := #OldToolLoc,
           Status                   := #Status,
           Ready                    := #Ready,
           Error                    := #Error);

      L     #Status; 
      T     "MW_Status_TM_TRANS"; 

      U     #Ready; 
      =     "MX_Ready_TM_TRANS"; 

      L     #Error; 
      T     "MW_ERROR_TM_TRANS"; 

