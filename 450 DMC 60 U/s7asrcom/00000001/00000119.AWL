      UN    "DB_ACHSE12".E_RefSyn1; 
      SPBN  n15a; 
      L     21; 
      T     "DB_ACHSE12".A_FD_OR; 
n15a: NOP   0; 

//---(1)--- Achsendlage ----------------------------------------------
      U     "I_WK_ENDSCHALTER_PLUS"; 
      =     "DB_ACHSE12".A_HWLimitPlus; 
      U     "I_WK_ENDSCHALTER_MINUS"; 
      =     "DB_ACHSE12".A_HWLimitMinus; 

//---(2)----U-Achse in Spindel----------------------------------------
      L     "DB_TOOL_DATA".SpWZ_Option; 
      L     1.000000e+000; 
      ==R   ; 
      UN(   ; 
      L     "DB_GREIFER_DATEN".TNR_Spindel; 
      L     0; 
      ==I   ; 
      )     ; 
      =     "m_UiSpi"; 
      =     "O_WK_KOPF_ANGEBAUT"; 

//---(3)-----Notbetrieb-----------------------------------------------
      U     "M_Reset_Taste"; 
      R     "DB_ANWENDER_SOFTKEY".U_Achse_Notbetrieb; 

      U     "DB_ANWENDER_SOFTKEY".U_Achse_Notbetrieb; 
      =     "m_UNotBt"; 

//---(4)-----Reglerfreigabe-------------------------------------------
      U     "DB_ACHSE12".A_ContrEnable; 
      =     "O_WK_REG_FREIGABE"; 


//---(5)-----U-Achse aktivieren über Taste Bedienpult-----------------
      U     E      4.3; 
      U     "M_Handbetrieb"; 
      U     "m_UiSpi"; 
      U     "DB_SIEM_KANAL_1".E_ChanReset; 
      FP    "M_FP_U_NotBetr"; 
      S     "Start_ASUP_Machine_Nr.4"; 

//---(6)-----Verfahrbereichsumschaltung sperren, wenn U-Achse aktiv ---
