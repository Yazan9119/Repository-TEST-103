      =     "DB_NC_PLC".Komet_UAchse_aktiv; 

//---(7)--- Schnittstelle zu Komet -----------------------------------
      CALL "FB_KOMET_U_ACHSE" , "DI_KOMET_U_ACHSE" (
           iEnable                  := "m_eins",
           iAlrm2e0                 := "I_WK_ALARMCODE_BIT_0",//Alarmcode Bit 0
           iAlrm2e1                 := "I_WK_ALARMCODE_BIT_1",//Alarmcode Bit 1
           iAlrm2e2                 := "I_WK_ALARMCODE_BIT_2",//Alarmcode Bit 2
           iAlrm2e3                 := "I_WK_ALARMCODE_BIT_3",//Alarmcode Bit 3
           iAlarm0                  := "I_WK_NEG_ALARM",//Alarmmeldung L-Aktiv
           iBusy0                   := "I_WK_NEG_WZ_AKTIV",//Werkzeug beschäftigt L-Aktiv
           iReady                   := "I_WK_WZ_BTB",//System bereit
