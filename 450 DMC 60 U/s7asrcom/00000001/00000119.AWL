           EX_Ax_E_LageRegler_61x5  := "DB_ACHSE12".E_PositContr,
           EX_Ax_E_IntSperr_93x6    := "DB_ACHSE12".E_IntegratDisable,
           EX_Ax_E_ImpFrei_93x7     := "DB_ACHSE12".E_PulseEnable,
