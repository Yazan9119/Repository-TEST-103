      UN    "DB_PLC_MD_DB20".UDHex._03_BIT3_ACHSE12_MESSYST; 
      U     "DB_ACHSE12".E_RefSyn1; 
      O     ; 
      U     "DB_PLC_MD_DB20".UDHex._03_BIT3_ACHSE12_MESSYST; 
      U     "DB_ACHSE12".E_RefSyn2; 
      =     "DB_ACHSE12".A_SWCam; 
NETWORK
TITLE =Clear TX_DUMMY

