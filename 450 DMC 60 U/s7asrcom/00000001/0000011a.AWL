      UN    "DB_PLC_MD_DB20".UDHex._03_BIT2_ACHSE11_MESSYST; 
      U     "DB_ACHSE11".E_RefSyn1; 
      O     ; 
      U     "DB_PLC_MD_DB20".UDHex._03_BIT2_ACHSE11_MESSYST; 
      U     "DB_ACHSE11".E_RefSyn2; 
      =     "DB_ACHSE11".A_SWCam; 
NETWORK
TITLE =Umladen Achstyp für (NC-Teilapperat)

      L     "MB_5Achs_Typ"; 
      T     "MB_NCTAchs_Typ"; 
      NOP   0; 
NETWORK
TITLE =Umladen Konfiguration für (NC-Teilapperat)

      L     "MB_5Achs_Konfig"; 
      T     "MB_NCTAchs_Konfig"; 
      NOP   0; 
NETWORK
