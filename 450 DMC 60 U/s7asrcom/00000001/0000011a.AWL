           EAX_Ax_A_LageMess1_1x5   := "DB_ACHSE11".A_PosMeas1,
           EAX_Ax_A_LageMess2_1x6   := "DB_ACHSE11".A_PosMeas2,
           EAX_Ax_A_EGUeberlag_4x5  := "DB_ACHSE11".A_RapidTrOR,
           EAX_Ax_A_RefVerzoeg_12x7 := "DB_ACHSE11".A_DelayRef,
           EAX_AX_A_Sp1Syn_16x4     := "DB_ACHSE11".A_Sp1Syn,
           EAX_Alarm_Regler         := "DB_FEHLERMELDUNG".Spi_VS_Halt._700126_11_IMP_REGFRG_0S,
