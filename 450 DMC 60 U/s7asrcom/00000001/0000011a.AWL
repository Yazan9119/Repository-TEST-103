           EX_Ax_E_StromRegler_61x7 := "DB_ACHSE11".E_CurrentContr,
           EX_Ax_E_FahrAF_61x0      := "DB_ACHSE11".E_TravRequ,
           EX_Ax_E_FBMinus_64x6     := "DB_ACHSE11".E_TCMinus,
